{
  "name": "kinvey-node-sdk",
  "description": "Kinvey JavaScript SDK for NodeJS applications.",
  "version": "5.0.0-next.0",
  "license": "Apache-2.0",
  "keywords": [
    "Kinvey",
    "JavaScript",
    "NodeJS"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "del lib && npm run lint",
    "build": "tsc -p tsconfig.json",
    "coverage": "nyc jest",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts}' --quiet --fix",
    "test": "jest",
    "test:watch": "mocha --watch --watch-extensions ts --opts mocha.opts"
  },
  "dependencies": {
    "js-base64": "^2.5.1",
    "lodash": "^4.17.11",
    "loglevel": "^1.6.3",
    "p-queue": "^6.0.2",
    "sift": "^8.4.0",
    "url": "^0.11.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@kinveysdk/eslint-config": "^0.0.1",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.133",
    "@types/nock": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "axios": "^0.19.0",
    "del-cli": "^1.1.0",
    "typescript": "^3.5.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0"
  },
  "files": [
    "lib/**/*"
  ]
}
